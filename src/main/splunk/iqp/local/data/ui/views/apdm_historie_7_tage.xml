<form script="apdm_historie/js/historie.js,apdm_historie/js/db_apdm_historie_letzte_7_tage.js" stylesheet="apdm_historie/css/db_apdm_historie_letzte_7_tage.css">
  <label>APDM Historie - letzte 7 Tage</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="dropdown" token="werkfilter" searchWhenChanged="true" id="werkinput">
      <label>Werk</label>
    </input>
    <input type="text" token="pruefumfang" searchWhenChanged="true">
      <label>Pruefumfang</label>
      <default>*</default>
    </input>
    <input type="text" token="pruefling" searchWhenChanged="true">
      <label>Pruefling</label>
      <default>*</default>
    </input>
    <input type="text" token="pruefprozedur" searchWhenChanged="true">
      <label>Pruefprozedur</label>
      <default>*</default>
    </input>
    <input type="text" token="sgbd" searchWhenChanged="true">
      <label>SGBD</label>
      <default>*</default>
    </input>
    <input type="text" token="errorCodeDec" searchWhenChanged="true">
      <label>Error Code (Dec)</label>
      <default>*</default>
    </input>
    <input type="text" token="resultData" searchWhenChanged="true">
      <label>Result Data</label>
      <default>*</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>APDM Fehler - letzte 7 Tage</title>
      
      <html>
        <div id="tbl_hist_7d"/> 
      </html>

    </panel>
  </row>
</form>