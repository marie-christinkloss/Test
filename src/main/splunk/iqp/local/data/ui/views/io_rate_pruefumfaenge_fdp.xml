  <row>
    <panel>
      <title>Direktläuferquote</title>
      <chart>
        <title>Prüfungen in der Montage und anschließenden Prüfstandslinie - Band 45: (A_CAS_INIT, A_DATA, A_DIAGNOSE), Band 53 (B_DATA, B_DIAGNOSE, EKS_INIT), Band 61 (F1_DIAGNOSE, VHT, ECOS, INFOTAINMENT), Prüfstandslinie (ROLLEPRFG, ABSPRFG, FINISH, MOTEST)</title>
        <search>
          <query>
| tstats 
    count AS "Anzahl von TestResult" 
  from datamodel=APDM_TestResults 
  where 
    (nodename = TestResult) 
    (TestResult.werk="$werkfilter$") 
    (TestResult.pruefumfangName=A_CAS_INIT OR TestResult.pruefumfangName=A_DATA OR TestResult.pruefumfangName=A_DIAGNOSE OR TestResult.pruefumfangName=B_DATA OR TestResult.pruefumfangName=B_DIAGNOSE OR TestResult.pruefumfangName=EKS_INIT OR TestResult.pruefumfangName=F1_DIAGNOSE OR TestResult.pruefumfangName=VHT OR TestResult.pruefumfangName=ECOS OR TestResult.pruefumfangName=INFOTAINMENT OR TestResult.pruefumfangName=ROLLEPRFG OR TestResult.pruefumfangName=ABSPRFG OR TestResult.pruefumfangName=FINISH OR TestResult.pruefumfangName=MOTEST) 
    (TestResult.inline=true) 
  groupby "TestResult.shortVIN", "TestResult.pruefumfangName", "TestResult.errorCount" 
| eval Band=if('TestResult.pruefumfangName'=="A_CAS_INIT" OR 'TestResult.pruefumfangName'=="A_DATA" OR 'TestResult.pruefumfangName'=="A_DIAGNOSE","Band45", if('TestResult.pruefumfangName'=="B_DATA" OR 'TestResult.pruefumfangName'=="B_DIAGNOSE" OR 'TestResult.pruefumfangName'=="EKS_INIT","Band53", if('TestResult.pruefumfangName'=="F1_DIAGNOSE" OR 'TestResult.pruefumfangName'=="VHT" OR 'TestResult.pruefumfangName'=="ECOS" OR 'TestResult.pruefumfangName'=="INFOTAINMENT","Band61", if('TestResult.pruefumfangName'=="ROLLEPRFG" OR 'TestResult.pruefumfangName'=="ABSPRFG" OR 'TestResult.pruefumfangName'=="FINISH" OR 'TestResult.pruefumfangName'=="MOTEST","Prüfstandslinie","-") ) ) ) 
| eval x=if('TestResult.errorCount'=="0","0","1") 
| stats max(x) as max, min(x) as min by TestResult.shortVIN, Band 
| eval direktlaeufer=if((max=="0" AND min="0"),"first_pass",if((max=="1" AND min="0"),"rework_in_line","rework_offline")) 
| fields - min max 
| stats count as gesamt, count(eval(direktlaeufer="first_pass")) as first_pass, count(eval(direktlaeufer="rework_in_line")) as rework_in_line, count(eval(direktlaeufer="rework_offline")) as rework_offline by Band 
| eval first_pass_percent=first_pass/gesamt*100 
| eval rework_in_line_percent=rework_in_line/gesamt*100 
| eval rework_offline_percent=rework_offline/gesamt*100 
| fields - first_pass rework_in_line rework_offline gesamt
          </query>
          <earliest>$timeselector.earliest$</earliest>
          <latest>$timeselector.latest$</latest>
        </search>
